<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
                  prism:ViewModelLocator.AutowireViewModel="True"
                  x:Class="OnlineShoppingTracker.Views.MainPage"
                  xmlns:prismNavigation="clr-namespace:Prism.Navigation.Xaml;assembly=Prism.Forms">
    <MasterDetailPage.Master>
        <ContentPage Title="Menu">
            <StackLayout Padding="20">
                <Button Text="Home" Command="{prismNavigation:NavigateTo 'MainPage/ProductStageTabbedPage'}" />
                <Button Text="Kept Items" Command="{prismNavigation:NavigateTo 'MainPage/NavigationPage/KeptListPage'}" />
            </StackLayout>
        </ContentPage>
    </MasterDetailPage.Master>
</MasterDetailPage>