<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  xmlns:prismNav="clr-namespace:Prism.Navigation.Xaml;assembly=Prism.Forms"
                  x:Class="OnlineShoppingTracker.Views.MainPage">
    <MasterDetailPage.Master>
        <ContentPage Title="Menu">
            <StackLayout Padding="20">
                <Button Text="Home" Command="{prismNav:NavigateTo 'StagesTabbedPage'}" />
                <Button Text="Kept Items" Command="{prismNav:NavigateTo 'NavigationPage/KeptListPage'}" />
            </StackLayout>
        </ContentPage>
    </MasterDetailPage.Master>
</MasterDetailPage>