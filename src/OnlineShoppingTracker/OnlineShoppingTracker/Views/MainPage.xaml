<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  x:Class="OnlineShoppingTracker.Views.MainPage">
    <MasterDetailPage.Master>
        <ContentPage Title="Menu">
            <StackLayout Padding="20">
                <Button Text="Home" Command="{Binding NavigateCommand}" CommandParameter="ProductStageTabbedPage" />
                <Button Text="Kept Items" Command="{Binding NavigateCommand}" CommandParameter="NavigationPage/KeptListPage" />
            </StackLayout>
        </ContentPage>
    </MasterDetailPage.Master>
</MasterDetailPage>